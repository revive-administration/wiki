package Ch08;

class Lamp {
	public void turnOn() {
		System.out.println("Lamp on");
	}
}

class Button {
	private Lamp theLamp;

	public Button(Lamp theLamp) {
		this.theLamp = theLamp;
	}

	public void pressed() {
		theLamp.turnOn();
	}
}

public class Client {
	public static void main(String[] args) {
		Lamp lamp = new Lamp();
		Button lampButton = new Button(lamp);
		lampButton.pressed();
	}

